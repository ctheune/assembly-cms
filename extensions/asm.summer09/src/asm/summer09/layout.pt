<script type="text/javascript">
var target;
var cookieName='ASSEMBLYsummer09siteguite';
function startClock() {
  target = document.getElementById('clock').getAttribute('alt');
  setTimeout('updateClock()',3000);
}
function updateClock() {
  var now = new Date();
  var diff = Math.floor((target-now)/1000);
  if (diff < 0) { diff=0; }
  document.getElementById('clock_days').innerHTML=Math.floor(diff/86400);
  diff=diff%86400;
  document.getElementById('clock_hours').innerHTML=Math.floor(diff/3600);
  diff=diff%3600;
  document.getElementById('clock_minutes').innerHTML=Math.floor(diff/60);
  if (diff > 0) { setTimeout('updateClock()',60000); }
}
function openguide() {
  new Effect.BlindDown('sguide-content');
  Element.toggle('sguide-bottom-show');
  Element.toggle('sguide-bottom-hide');
  return false;
}
function closeguide() {
  new Effect.BlindUp('sguide-content');
  Element.toggle('sguide-bottom-show');
  Element.toggle('sguide-bottom-hide');
  createCookie(cookieName,'seen',300);
  return false;
}

var newsRotation=true;
var currentNews;
function doStartupStuff() {
  var cookie = readCookie(cookieName);
  if (cookie==null) {
    openguide();
    setTimeout('autocloseguide()',10000);
  }
  currentNews="news-id1";  
  setTimeout('rotateNews()',10000);
}
function createCookie(name,value,days)
{
  if (days)
  {
    var date = new Date();
    date.setTime(date.getTime()+(days*24*60*60*1000));
    var expires = "; expires="+date.toGMTString();
  }
  else var expires = "";
  document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name)
{
  var nameEQ = name + "=";
  var ca = document.cookie.split(';');
  for(var i=0;i < ca.length;i++)
  {
    var c = ca[i];
    while (c.charAt(0)==' ') c = c.substring(1,c.length);
    if (c.indexOf(nameEQ) == 0) { return c.substring(nameEQ.length,c.length); }
  }
  return null;
}
function eraseCookie(name)
{
  createCookie(name,"",-1);
}
function switch_news(div) {
    var old = currentNews;
    if (old == div) { return false; }
    if (!document.getElementById(div)) { return false; }
    var elem=document.getElementById(div+'-nav');
    if (!Element.hasClassName(elem,"active")) {
        Element.addClassName(elem,"active");
    }
    document.getElementById(div).style.display="block";
    document.getElementById(old).style.display="none";
    Element.removeClassName(document.getElementById(old+'-nav'),"active");
    currentNews=div;
    newsRotation=false;
    return false;
}
function switch_news_next() {
    var old = currentNews;
    var num = old.substring(7) * 1;
    num +=1;
    if (!document.getElementById('news-id'+num)) { num=1; }
    switch_news('news-id'+num);
    return false;
}
function switch_news_prev() {
    var old = currentNews;
    var num = old.substring(7) * 1;
    num -=1;
    if (num==0) {
        num+=1;
        while(document.getElementById('news-id'+num)) { num+=1; }
        num-=1;
    }
    switch_news('news-id'+num);
    return false;
}
function rotateNews() {
    if (newsRotation) {
        switch_news_next();
        newsRotation=true;
        setTimeout('rotateNews()',7000);
    }
}
function stopNews() {
    newsRotation=false;
}
</script>


