<h1 tal:content="context/title">Year</h1>

<div tal:repeat="category view/list_categories">
  <h2><a tal:attributes="href python:view.url(category.page)"
         tal:content="category/title">Compo type</a></h2>

  <table tal:repeat="item python:view.list_category_items(category, 3)" style="display: inline">
  <tr><td>
    <div>
      <a tal:attributes="href python:view.url(item)">
        <img tal:attributes="src python:'://' in item.thumbnail and item.thumbnail or view.url(item) + '/' + item.thumbnail;
                         alt item/title" />
      </a>
  </td></tr>
  <tr><td>
    <a tal:attributes="href python:view.url(item)">
      <span tal:replace="item/title" />
    </a>
  </td></tr>
  </table>
</div>