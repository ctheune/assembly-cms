<tal:block tal:define="items view/tree">

  <ul id="navigation">
      <li tal:repeat="item items"
          talattributes="class item/class">
          <a talattributes="href python:view.url(item['page']);
                             class python:view.page is item['page'] and 'selected' or ' '"
             tal:content="python:item['page'].title"></a>
      </li>
  </ul>

</tal:block>
