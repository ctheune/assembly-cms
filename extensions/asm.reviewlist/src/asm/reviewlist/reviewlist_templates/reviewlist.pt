<div class="section">
<h2>Recent changes</h2>
</div>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <th>Page</th>
      <th>User</th>
    </tr>
  <tbody>
    <tr tal:repeat="change view/changes">
      <td tal:content="change/timestamp/@@format/when"></td>
      <td><a tal:attributes="href python:view.url(change.object)"
             tal:content="change/object/title"></a></td>
      <td tal:content="change/user_id"></td>
    </tr>
  </tbody>
</table>
