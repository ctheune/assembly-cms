<tal:block tal:define="items view/tree">
  <div class="nav-collapse">
    <ul class="nav" tal:define="items view/tree">
      <li tal:repeat="item items"
          tal:attributes="class python:view.page is item['page'] and 'active' or ' '"><a tal:attributes="href python:view.url(item['page'])" tal:content="item/page/title">About</a></li>
    </ul>
  </div><!--/.nav-collapse -->
</tal:block>
